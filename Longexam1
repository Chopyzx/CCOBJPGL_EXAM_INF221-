import Locations.*;
import Tourists.*;



public class App {
    public static void main(String[] args) throws Exception {

        Locations Bohol = new Bohol();
        Locations Boracay = new Boracay();
        Locations Cebu = new Cebu();
        Locations Palawan = new Palawan();
        Locations Siargao = new Siargao();
        Locations Vigan = new Vigan();

        Tourist Arjay = new Arjay();

        Boracay.accept(Arjay);
        Bohol.accept(Arjay);
        Cebu.accept(Arjay);
        Palawan.accept(Arjay);
        Siargao.accept(Arjay);
        Vigan.accept(Arjay);



    }
}
